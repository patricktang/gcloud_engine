{
  "index_patterns": [
    "devweb*"
  ],
  "order": 1,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "mappings": {
    "_source": {
      "enabled": true
    },
    "dynamic_templates": [
      {
        "message_field": {
          "mapping": {
            "norms": false,
            "type": "keyword"
          },
          "match_mapping_type": "string",
          "match": "message"
        }
      },
      {
        "string_fields": {
          "mapping": {
            "type": "keyword",
            "norms": false,
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "match_mapping_type": "string",
          "match": "*"
        }
      }
    ],
    "properties": {
      "geoip": {
        "dynamic": true,
        "properties": {
          "location": {
            "type": "geo_point"
          }
        },
        "type": "object"
      },
      "request": {
        "type": "keyword"
      }
    }
  }
}
